    width: 300px;
    height: 100px;
    align-items: center;
    display: flex;
}

.outersection{
    display: flex;
    flex-direction: column;
}
.header>h1{
   width: 200px;
}

.autoplay{
 height:270px;
 margin:auto;
 width:90%;
 }
.autoplay .image img
{
 width:100%;
 height:100%;
}
.autoplay .image{
width:250px;
height:190px;
display:flex;
align-items:center;
}

.header{
    height: 45px;
    background-color: darkslategrey;
    display: flex;
    align-items: center;
    width: 86vw;
    color: wheat;
    margin: 0 auto;
    padding: 10px;
    justify-content: center;
    border-radius: 5px;
    }
.container{
        display:grid;
        grid-template-columns: repeat(3,150px);
        grid-template-rows: repeat(8,300px);
        grid-gap: 10px;
    width: 100%;
 
        border-bottom: 1px solid wheat;
      
}
.para {
    width: 450px;
    padding: 10px;
    color: wheat;
margin-top: 50px;
    margin-right: auto;
    margin-bottom: 20px;
    margin-left:auto;
    line-height: 40px;
    background-color: darkslategray;
}
.footer-container{
    background-color: darkslategrey;

}
.section>img{
    display:grid; 
    width:150px;
    height:100px;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    }
    .section>li{
    list-style-type:none;
    padding:0;
    margin-top: 5px;
    width: 150px;
    font-size: smaller;
    color:yellow;
    background-color: rgb(2, 48, 48);
    }
    .section>li>span{
   text-align:right;
    list-style-type:none;
    padding:0;
    margin-top: 5px;
    width: 150px;
    color:whitesmoke;
    font-size:15px;
    }
.innerfooter>ul>li {
   padding: 0px; 
   list-style-type: none; 
}

.center{
    margin: auto;
}
.center .leftsearch form select{
    margin-left:5px;

}
.center{
     width: 75vw;
    margin: auto;
    }
.center .mainsearch form{
        display: flex;
        box-shadow: 3px 3px 5px #631818;
        align-items: center;
      justify-content: center;
    }    
    .center .leftsearch form{
        display: flex;
      justify-content: center;
    }	

.footer-container{
    display: flex;
    justify-content: center;
    flex-direction: column;
    width: 88vw;
    background-color:darkslategrey;
    border-radius: 5px;

}
